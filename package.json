		"colors": [
			{
				"id": "creatio.startIcon",
				"description": "Color for the start icon tree item in the custom view.",
				"defaults": {
					"dark": "#00ff00",
					"light": "#00ff00",
					"highContrast": "#00ff00"
				}
			},
			{
				"id": "creatio.blue",
				"description": "Color for the business process item in the tree view.",
				"defaults": {
					"dark": "#007acc",
					"light": "#007acc",
					"highContrast": "#007acc"
				}
			},
			{
				"id": "creatio.orangePrimary",
				"description": "Color for the business process item in the tree view.",
				"defaults": {
					"dark": "#ed522f",
					"light": "#ed522f",
					"highContrast": "#ed522f"
				}
			},
			{
				"id": "creatio.orangeSecondary",
				"description": "Color for the business process item in the tree view.",
				"defaults": {
					"dark": "#77574f",
					"light": "#77574f",
					"highContrast": "#77574f"
				}
			},
			{
				"id": "creatio.orangeTertiary",
				"description": "Color for the business process item in the tree view.",
				"defaults": {
					"dark": "#6d5d2e",
					"light": "#6d5d2e",
					"highContrast": "#6d5d2e"
				}
			}
		],
		"configuration": [
			{
				"title": "Clio explorer",
				"properties": {
					"archivePath": {
						"type": "string",
						"default": "C:\\Build",
						"description": "Archive path"
					},
					"installRoot": {
						"type": "string",
						"default": "C:\\inetpub\\wwwroot",
						"description": "Install root directory"
					},
					"clio.autoUpdateCli": {
						"type": "boolean",
						"default": true,
						"description": "Perform clio update when a newer version is available, or when clio is not installed"
					},

					"templatePrefix": {
						"type": "string",
