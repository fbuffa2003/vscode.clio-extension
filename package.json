				}
			},
			{
				"command": "ClioSQL.InstallGate",
				"title": "Install clio api",
				"icon": {
					"light": "resources/icon/Health.svg",
					"dark": "resources/icon/Health.svg"
				}
			},
			{
				"command": "ClioSQL.UpdateClioCli",
				"title": "Install/Update clio cli",
